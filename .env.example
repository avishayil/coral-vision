# Coral Vision Environment Variables

# Data Directory
DATA_DIR=./data              # Path to data directory (models, labels)

# Database Configuration (pgvector backend)
DB_HOST=localhost            # PostgreSQL host
DB_PORT=5432                 # PostgreSQL port
DB_NAME=coral_vision         # Database name
DB_USER=coral                # Database user
DB_PASSWORD=coral            # Database password

# Edge TPU Configuration
USE_EDGETPU=false            # Set to 'true' to use Coral Edge TPU

# Web Server Configuration
HOST=0.0.0.0                 # Server host
PORT=5000                    # Server port
DEBUG=false                  # Debug mode

API_KEY=xxxxxxxxxxxxxxxx